class Solution {
    public boolean checkIfPangram(String sentence) 
    {
      Set<Character>set=new HashSet<Character>();
      int n = sentence.length();
      for(int i=0;i<n;i++)
      {
        char c = sentence.charAt(i);
        if(c>='a' && c<='z')
         set.add(c);
        
      }  
      return set.size()==26;
    }
}
